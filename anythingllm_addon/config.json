{
  "name": "AnythingLLM",
  "version": "0.1",
  "slug": "anythingllm",
  "description": "Run AnythingLLM inside Home Assistant and expose its web GUI.",
  "url": "https://github.com/hassio-addons/addon-adguard-home",
  "webui": "http://[HOST]:[PORT:8501]",
  "ingress": true,
  "ingress_port": 0,
  "panel_icon": "mdi:shield-check",
  "startup": "services",
  "homeassistant": "0.101.0b0",
  "arch": [
    "aarch64",
    "amd64",
    "armhf",
    "armv7",
    "i386"
  ],
  "ports": {
    "8501/tcp": 8501
  },
  "ports_description": {
    "8501/tcp": "Web interface (Not required for Ingress)"
  },
  "discovery": [
    "anythingllm"
  ],
  "boot": "auto",
  "hassio_api": true,
  "hassio_role": "default",
  "auth_api": true,
  "host_network": true,
  "map": [
    "ssl"
  ],
  "options": {
    "model_path": "/config/models",
    "log_level": "info"
  },
  "schema": {
    "model_path": "?str",
    "log_level": "?list(info|debug|warning|error)"
  },
  "image": "nubicula/anythingllm_addon"
}